{
  "name": "logistix",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run db:initialize && ts-node scripts/ensure-port-free.ts 3000 && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit --project tsconfig.check.json",
    "checks": "npm run typecheck && npm run lint",
    "fix:sqlite3": "bash scripts/fix-sqlite3.sh",
    "db:initialize": "npx tsx ./scripts/db/initialize.ts",
    "db:generate": "ts-node scripts/db/generate.ts",
    "db:migrate": "npx tsx scripts/db/migrate.ts",
    "db:seed": "npx tsx scripts/db/seed.ts",
    "db:studio": "npx drizzle-kit studio --config ./drizzle.config.json",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit --passWithNoTests",
    "test:integration": "vitest run tests/integration --passWithNoTests",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "analyze:unused": "knip",
    "superbuy:extract": "bash scripts/run-in-venv.sh npx tsx scripts/superbuy/index.ts",
    "superbuy:login": "npx tsx scripts/superbuy-headful-login.ts",
    "captcha:prepare-dataset": "cd captcha-solver-api/training && python prepare_dataset.py",
    "captcha:train-beginner": "cd captcha-solver-api/training && python train_wrapper.py --config beginner",
    "captcha:train-advanced": "cd captcha-solver-api/training && python train_wrapper.py --config advanced",
    "captcha:start": "cd captcha-solver-api && source venv/bin/activate && python -m uvicorn app.main:app --reload --port 8000",
    "captcha:docker-up": "docker-compose -f docker-compose.captcha.yml up -d",
    "captcha:docker-down": "docker-compose -f docker-compose.captcha.yml down",
    "captcha:docker-logs": "docker-compose -f docker-compose.captcha.yml logs -f captcha-api",
    "captcha:test": "cd captcha-solver-api && ./run_test.sh",
    "captcha:active-learning": "npx tsx scripts/captcha/active-learning.ts",
    "tidy": "bash scripts/tidy.sh"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
