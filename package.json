    "onnxruntime-node": "^1.23.2",
    "pg": "^8.16.3",
    "playwright": "^1.55.0",
    "pngjs": "^7.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "recharts": "^2.10.3",
    "server-only": "^0.0.1",
    "sharp": "^0.34.5",
    "sonner": "^2.0.6",
    "tailwind-merge": "^2.1.0",
