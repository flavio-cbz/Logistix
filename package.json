{
  "name": "logistix",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run db:initialize && ts-node scripts/maintenance/ensure-port-free.ts 3000 && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit --project tsconfig.check.json",
    "checks": "npm run typecheck && npm run lint",
    "db:initialize": "npx tsx ./scripts/db/initialize.ts",
    "db:generate": "ts-node scripts/db/generate.ts",
    "db:migrate": "npx tsx scripts/db/migrate.ts",
    "db:seed": "npx tsx scripts/db/seed.ts",
    "db:studio": "npx drizzle-kit studio --config ./drizzle.config.json",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit --passWithNoTests",
    "test:integration": "vitest run tests/integration --passWithNoTests",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "analyze:unused": "knip",
    "superbuy:login": "npx tsx scripts/superbuy/superbuy-headful-login.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@tanstack/react-query": "^5.90.20",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "cmdk": "^1.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "framer-motion": "^12.29.2",
    "lucide-react": "^0.563.0",
    "nanoid": "^5.1.6",
    "next": "^16.1.6",
    "next-themes": "^0.4.6",
    "onnxruntime-node": "^1.23.2",
    "pg": "^8.17.2",
    "pngjs": "^7.0.0",
    "react": "^19.2.4",
    "react-day-picker": "^9.13.0",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.71.1",
    "recharts": "^3.7.0",
    "sharp": "^0.34.5",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^13.0.0",
    "vinted": "github:remax21/vinted-api",
    "vinted-api": "^2.9.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.6",
    "@playwright/test": "^1.58.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.2",
    "@types/bcrypt": "^6.0.0",
    "@types/better-sqlite3": "^7.6.8",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.1.0",
    "@types/pg": "^8.16.0",
    "@types/pngjs": "^6.0.5",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.16",
    "autoprefixer": "^10.4.23",
    "drizzle-kit": "^0.31.7",
    "eslint": "^9.39.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "knip": "^5.82.1",
    "lint-staged": "^16.2.7",
    "node-mocks-http": "^1.17.2",
    "playwright": "^1.55.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "tsc-files": "^1.1.4",
    "tslib": "^2.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5.8.3",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.0.5",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "overrides": {
    "@npmcli/move-file": "^2.0.0",
    "glob": "^11.0.0",
    "rimraf": "3.0.2",
    "esbuild": "^0.27.2",
    "diff": "^8.0.3"
  },
  "bundlesize": [
    {
      "path": ".next/static/js/**/*.js",
      "maxSize": "300 kB"
    },
    {
      "path": ".next/static/css/**/*.css",
      "maxSize": "50 kB"
    }
  ],
  "description": "LogistiX — Outil d'analyse et d'automatisation pour la marketplace Vinted : collecte, normalization et scoring d'annonces pour analyses de marché et actions automatisées.",
  "main": "next.config.mjs",
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flavio-cbz/Logistix.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/flavio-cbz/Logistix/issues"
  },
  "homepage": "https://github.com/flavio-cbz/Logistix#readme",
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "tsc-files --noEmit"
    ],
    "*.{js,jsx}": [
      "eslint --fix"
    ]
  }
}
